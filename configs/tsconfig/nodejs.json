{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "Display": "Node.js",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "types": ["node"],
    "importHelpers": false,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*.ts"]
}
